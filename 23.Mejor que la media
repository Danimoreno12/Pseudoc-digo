
Algoritmo sin_titulo
	Dimension Vector[10]
	Para i<-1 Hasta 10  Hacer
		Vector(i)=Aleatorio(1,20)
		Escribir vector(i) Sin Saltar ", "
	Fin Para
	
	suma=0
	Para i<-1 Hasta 10  Hacer
		suma=vector(i)+suma
	Fin Para
	Escribir "La media es " suma/10 
	
	Para i<-1 Hasta 10  Hacer
		si vector(i)<media
			Escribir "El numero " vector(i) " se encuentra por debajo de la media"
		SiNo
			Escribir "El numero " vector(i) " se encuentra por encima de la media"
		FinSi
	Fin Para
	
FinAlgoritmo
